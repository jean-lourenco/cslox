expression -> comma
comma - > equality ("," equality)*
equality -> comparision(("==" | "!=") comparision)*
comparision -> term ((">" | "<" | ">=" | "<=") term)*
term -> factor(("+" | "-") factor)*
factor -> unary(("*" | "/") unary)*
unary -> ("-" | "!") unary | primary
primary -> NUMBER | STRING | "nil" | "true" | "false" | "(" expression ")"